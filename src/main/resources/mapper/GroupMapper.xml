<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhrenjie04.alex.user.dao.GroupDao">
	<!-- cache type="org.mybatis.caches.memcached.MemcachedCache" / -->
    <resultMap type="com.zhrenjie04.alex.user.domain.Group"
        id="groupResult">
        <result property="groupId" column="group_id" />
        <result property="groupName" column="group_name" />
        <result property="groupCode" column="group_code" />
        <result property="parentId" column="parent_id" />
        <result property="idPath" column="id_path" />
        <result property="isLocked" column="is_locked" />
        <result property="isDeleted" column="is_deleted" />
        <result property="createdTime" column="created_time" />
        <result property="createrId" column="creater_id" />
        <result property="createrName" column="creater_name" />
        <result property="lastModifiedTime" column="last_modified_time" />
        <result property="lastModifierId" column="last_modifier_id" />
        <result property="lastModifierName" column="last_modifier_name" />
    </resultMap>
    <select id="queryObjectById" resultMap="groupResult" parameterType="java.lang.String">
        select * from t_group where group_id=#{id}
    </select>
</mapper> 