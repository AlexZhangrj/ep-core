<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhrenjie04.alex.user.dao.IdentityId2RoleIdDao">
	<!-- cache type="org.mybatis.caches.memcached.MemcachedCache" / -->
    <resultMap type="com.zhrenjie04.alex.user.domain.IdentityId2RoleId"
        id="entityResult">
        <result property="id" column="id" />
        <result property="identityId" column="identity_id" />
        <result property="groupId" column="group_id" />
        <result property="roleId" column="role_id" />
        <result property="userId" column="user_id" />
        <result property="isLocked" column="is_locked" />
        <result property="isDeleted" column="is_deleted" />
        <result property="createdTime" column="created_time" />
        <result property="createrId" column="creater_id" />
        <result property="createrName" column="creater_name" />
        <result property="lastModifiedTime" column="last_modified_time" />
        <result property="lastModifierId" column="last_modifier_id" />
        <result property="lastModifierName" column="last_modifier_name" />
    </resultMap>
    <select id="queryAllByIdentityId" resultMap="entityResult" parameterType="java.lang.String">
        select * from t_privilege where privilege_id in(select privilege_id from t_role_privilege where role_id=#{id})
    </select>
</mapper> 
