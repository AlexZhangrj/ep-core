<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhrenjie04.alex.user.dao.IdentityDao">
	<!-- cache type="org.mybatis.caches.memcached.MemcachedCache" / -->
    <resultMap type="com.zhrenjie04.alex.core.Identity"
        id="identityResult">
        <result property="identityId" column="identity_id" />
        <result property="userId" column="user_id" />
        <result property="groupId" column="group_id" />
        <result property="positionId" column="position_id" />
        <result property="isLocked" column="is_locked" />
        <result property="isDeleted" column="is_deleted" />
        <result property="created_time" column="created_time" />
        <result property="creater_id" column="creater_id" />
        <result property="creater_name" column="creater_name" />
        <result property="last_modified_time" column="last_modified_time" />
        <result property="last_modifier_id" column="last_modifier_id" />
        <result property="last_modifier_name" column="last_modifier_name" />
    </resultMap>
    <select id="queryListByUserId" resultMap="identityResult" parameterType="java.lang.String">
        select * from t_identity where user_id=#{id}
    </select>
</mapper> 